    urlData = urlData.replace('"https://services.nofraud.com/js/device.js?shop=gulfcoastavionics.myshopify.com",',''); // yet to include
    urlData = urlData.replace(',"https://cdn.joinclyde.com/scripts/shopify-storefront.min.js?shop=gulfcoastavionics.myshopify.com"',''); 
    urlData = urlData.split(',');
    
    for (i = 0; i < urlData.length; i++) {
      var linkUrl = urlData[i].replace(/"/g,'');
      var scriptEle = document.createElement( 'script');
      scriptEle.defer = true;
      scriptEle.setAttribute( 'src', linkUrl );
      document.head.appendChild(scriptEle);
    }
    
    {% if template.name == 'product' %}
    
    var generateBundleJS = document.createElement( 'script');
    generateBundleJS.setAttribute( 'src', '//secure.apps.shappify.com/apps/bundle/generate_bundle.php?shop=gulfcoastavionics.myshopify.com' );
    generateBundleJS.defer = true;
    document.body.appendChild(generateBundleJS);
    
