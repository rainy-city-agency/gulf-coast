    urlData = urlData.replace('"//secure.apps.shappify.com/apps/bundle/generate_bundle.php?shop=gulfcoastavionics.myshopify.com",',''); 
    urlData = urlData.replace('"//bundles.boldapps.net/bundles_install_check.js?shop=gulfcoastavionics.myshopify.com",',''); 
