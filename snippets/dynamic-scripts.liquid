    {% if settings.enable_google_translator %}
    let paymentLoaded = false;
    const addTranslationScript = setInterval(function(){
      const checkoutButtons = document.getElementById('dynamic-checkout-cart');
      if(checkoutButtons) paymentLoaded = true;
      if(paymentLoaded == true){        
        var scriptT = document.createElement( 'script');
        scriptT.setAttribute( 'src', '//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit' );
        document.body.appendChild(scriptT);
        clearInterval(addTranslationScript);
      }     
