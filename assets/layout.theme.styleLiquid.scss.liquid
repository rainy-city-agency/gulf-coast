@font-face {
  font-family: "gca-icon";
  src:url("../assets/gca-icon.eot");
  src:url("../assets/gca-icon.eot?#iefix") format("embedded-opentype"),
    url("../assets/gca-icon.woff") format("woff"),
    url("../assets/gca-icon.ttf") format("truetype"),
    url("../assets/gca-icon.svg#gca-icon") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "gca-icon" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "gca-icon" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-angle-right:before {
  content: "\62";
}
.icon-angle-down:before {
  content: "\63";
}
.icon-angle-left:before {
  content: "\64";
}
.icon-angle-up:before {
  content: "\65";
}
.icon-user:before {
  content: "\61";
}
.icon-wishlist:before {
  content: "\66";
}
.icon-cart:before {
  content: "\67";
}
.icon-arrow-rg:before {
  content: "\68";
}
.icon-arrow-lft:before {
  content: "\69";
}
.icon-search:before {
  content: "\6a";
}
.icon-news-arrow:before {
  content: "\6b";
}
.icon-search{
  color: #9B9C9A;
}

